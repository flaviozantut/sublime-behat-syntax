<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>fileTypes</key>
    <array>
        <string>feature</string>
    </array>
    <key>firstLineMatch</key>
    <string>^(Feature|Funcionalidade):(.+)$</string>
    <key>foldingStartMarker</key>
    <string>((Feature|Funcionalidade)\:|(Background|Contexto|Cen(a|á)rio de Fundo|Fundo)\:|(Scenario|Cen(á|a)rio) Outline\:|(Scenario|Cen(á|a)rio)\:|Examples\:)</string>
    <key>foldingStopMarker</key>
    <string>^(\r\n|\n)$</string>
    <key>keyEquivalent</key>
    <string>^~B</string>
    <key>name</key>
    <string>Behat</string>
    <key>patterns</key>
    <array>
        <dict>
            <key>match</key>
            <string>^\s+\b(And|Given|Then|When|But|Dado|Dada|Dados|Dadas|Ent(ã|a)o|E|Quando|Mas)\b</string>
            <key>name</key>
            <string>keyword.control.elements.behat</string>
        </dict>
        <dict>
            <key>match</key>
            <string>((Feature|Funcionalidade)\:|(Background|Contexto|Cen(a|á)rio de Fundo|Fundo)\:|(Scenario|Cen(á|a)rio) Outline\:|(Scenario|Cen(á|a)rio)\:|Examples\:)</string>
            <key>name</key>
            <string>keyword.control.feature.behat</string>
        </dict>
        <dict>
            <key>match</key>
            <string>\b(CONNECT|DELETE|GET|HEAD|OPTIONS|PATCH|POST|PUT|TRACE)\b</string>
            <key>name</key>
            <string>keyword.http.verbs.behat</string>
        </dict>
        <dict>
            <key>patterns</key>
            <array>
                <dict>
                    <key>include</key>
                    <string>#strings</string>
                </dict>
                <dict>
                    <key>include</key>
                    <string>#numbers</string>
                </dict>
            </array>
        </dict>
    </array>
    <key>repository</key>
    <dict>
        <key>comment</key>
        <dict>
            <key>patterns</key>
            <array>
                <dict>
                    <key>captures</key>
                    <dict>
                        <key>1</key>
                        <dict>
                            <key>name</key>
                            <string>punctuation.definition.comment.behat</string>
                        </dict>
                    </dict>
                    <key>match</key>
                    <string>(?&lt;!\S)(#)(?!\{).*$\n?</string>
                    <key>name</key>
                    <string>comment.line.number-sign.behat</string>
                </dict>
            </array>
        </dict>
        <key>regex-double-quoted</key>
        <dict>
            <key>begin</key>
            <string>(?x)"/ (?= (\\.|[^"/])++/[imsxeADSUXu]*" )</string>
            <key>beginCaptures</key>
            <dict>
                <key>0</key>
                <dict>
                    <key>name</key>
                    <string>punctuation.definition.string.begin.behat</string>
                </dict>
            </dict>
            <key>end</key>
            <string>(/)([imsxeADSUXu]*)(")</string>
            <key>endCaptures</key>
            <dict>
                <key>0</key>
                <dict>
                    <key>name</key>
                    <string>punctuation.definition.string.end.behat</string>
                </dict>
            </dict>
            <key>name</key>
            <string>string.regexp.double-quoted.behat</string>
            <key>patterns</key>
            <array>
                <dict>
                    <key>comment</key>
                    <string>Escaped from the regexp – there can also be 2 backslashes (since 1 will escape the first)</string>
                    <key>match</key>
                    <string>(\\){1,2}[.$^\[\]{}]</string>
                    <key>name</key>
                    <string>constant.character.escape.regex.behat</string>
                </dict>
                <dict>
                    <key>include</key>
                    <string>#interpolation</string>
                </dict>
                <dict>
                    <key>captures</key>
                    <dict>
                        <key>1</key>
                        <dict>
                            <key>name</key>
                            <string>punctuation.definition.arbitrary-repitition.behat</string>
                        </dict>
                        <key>3</key>
                        <dict>
                            <key>name</key>
                            <string>punctuation.definition.arbitrary-repitition.behat</string>
                        </dict>
                    </dict>
                    <key>match</key>
                    <string>(\{)\d+(,\d+)?(\})</string>
                    <key>name</key>
                    <string>string.regexp.arbitrary-repitition.behat</string>
                </dict>
                <dict>
                    <key>begin</key>
                    <string>\[(?:\^?\])?</string>
                    <key>captures</key>
                    <dict>
                        <key>0</key>
                        <dict>
                            <key>name</key>
                            <string>punctuation.definition.character-class.behat</string>
                        </dict>
                    </dict>
                    <key>end</key>
                    <string>\]</string>
                    <key>name</key>
                    <string>string.regexp.character-class.behat</string>
                    <key>patterns</key>
                    <array>
                        <dict>
                            <key>include</key>
                            <string>#interpolation</string>
                        </dict>
                    </array>
                </dict>
                <dict>
                    <key>match</key>
                    <string>[$^+*]</string>
                    <key>name</key>
                    <string>keyword.operator.regexp.behat</string>
                </dict>
            </array>
        </dict>
        <key>string-double-quoted</key>
        <dict>
            <key>begin</key>
            <string>"</string>
            <key>beginCaptures</key>
            <dict>
                <key>0</key>
                <dict>
                    <key>name</key>
                    <string>punctuation.definition.string.begin.behat</string>
                </dict>
            </dict>
            <key>comment</key>
            <string>This contentName is just to allow the usage of “select scope” to select the string contents first, then the string with quotes</string>
            <key>contentName</key>
            <string>meta.string-contents.quoted.double.behat</string>
            <key>end</key>
            <string>"</string>
            <key>endCaptures</key>
            <dict>
                <key>0</key>
                <dict>
                    <key>name</key>
                    <string>punctuation.definition.string.end.behat</string>
                </dict>
            </dict>
            <key>name</key>
            <string>string.quoted.double.behat</string>
            <key>patterns</key>
            <array>
                <dict>
                    <key>include</key>
                    <string>#interpolation</string>
                </dict>
            </array>
        </dict>
        <key>string-examples</key>
        <dict>
            <key>begin</key>
            <string>(\| )</string>
            <key>beginCaptures</key>
            <dict>
                <key>0</key>
                <dict>
                    <key>name</key>
                    <string>punctuation.definition.string.begin.behat</string>
                </dict>
            </dict>
            <key>comment</key>
            <string>This contentName is just to allow the usage of “select scope” to select the string contents first, then the string with quotes</string>
            <key>contentName</key>
            <string>meta.string-contents.examples.behat</string>
            <key>end</key>
            <string>( \|\r)</string>
            <key>endCaptures</key>
            <dict>
                <key>0</key>
                <dict>
                    <key>name</key>
                    <string>punctuation.definition.string.end.behat</string>
                </dict>
            </dict>
            <key>name</key>
            <string>string.examples.behat</string>
            <key>patterns</key>
            <array>
                <dict>
                    <key>include</key>
                    <string>#interpolation</string>
                </dict>
            </array>
        </dict>
        <key>string-placeholders</key>
        <dict>
            <key>begin</key>
            <string>&lt;</string>
            <key>beginCaptures</key>
            <dict>
                <key>0</key>
                <dict>
                    <key>name</key>
                    <string>punctuation.definition.string.begin.behat</string>
                </dict>
            </dict>
            <key>comment</key>
            <string>This contentName is just to allow the usage of “select scope” to select the string contents first, then the string with quotes</string>
            <key>contentName</key>
            <string>meta.string-contents.placeholders.behat</string>
            <key>end</key>
            <string>&gt;</string>
            <key>endCaptures</key>
            <dict>
                <key>0</key>
                <dict>
                    <key>name</key>
                    <string>punctuation.definition.string.end.behat</string>
                </dict>
            </dict>
            <key>name</key>
            <string>string.placeholders.behat</string>
            <key>patterns</key>
            <array>
                <dict>
                    <key>include</key>
                    <string>#interpolation</string>
                </dict>
            </array>
        </dict>
        <key>numbers</key>
        <dict>
            <key>match</key>
            <string>\b((0(x|X)[0-9a-fA-F]*)|(([0-9]+\.?[0-9]*)|(\.[0-9]+))((e|E)(\+|-)?[0-9]+)?)\b</string>
            <key>name</key>
            <string>constant.numeric.behat</string>
        </dict>
        <key>strings</key>
        <dict>
            <key>patterns</key>
            <array>
                <dict>
                    <key>include</key>
                    <string>#regex-double-quoted</string>
                </dict>
                <dict>
                    <key>include</key>
                    <string>#string-examples</string>
                </dict>
                <dict>
                    <key>include</key>
                    <string>#string-placeholders</string>
                </dict>                <dict>
                    <key>include</key>
                    <string>#string-double-quoted</string>
                </dict>
                <dict>
                    <key>include</key>
                    <string>#comment</string>
                </dict>
            </array>
        </dict>
    </dict>
    <key>scopeName</key>
    <string>text.feature</string>
</dict>
</plist>
